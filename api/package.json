{
	"name": "api",
	"version": "1.0.0",
	"main": "index.ts",
	"license": "MIT",
	"scripts": {
		"build": "npx tsc",
		"start": "node build/index.js",
		"start-silent": "nodemon -q index.ts",
		"dev": "concurrently \"npx tsc --watch\" \"nodemon -q build/index.js\"",
		"dev-ts": "ts-node index.ts"
	},
	"nodemonConfig": {
		"watch": "./**/*.ts",
		"execMap": {
			"ts": "ts-node"
		}
	},
	"devDependencies": {
		"@types/bluebird": "^3.5.37",
		"@types/cookie-parser": "^1.4.3",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.14",
		"@types/node": "^18.8.4",
		"@types/passport": "^1.0.11",
		"@types/passport-jwt": "^3.0.7",
		"concurrently": "^7.6.0",
		"nodemon": "^2.0.20",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.0",
		"tsup": "^6.2.3",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"@types/bcrypt": "^5.0.0",
		"bcrypt": "^5.1.0",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"jsonwebtoken": "^8.5.1",
		"mysql2": "^2.3.3",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.0",
		"sequelize": "^6.25.0"
	}
}